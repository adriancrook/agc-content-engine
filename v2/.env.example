# AGC v2 Configuration

# Database (use Railway Postgres or local SQLite)
DATABASE_URL=sqlite:///agc_v2.db
# DATABASE_URL=postgresql://user:pass@host:port/dbname

# Enable real agents (requires Ollama running)
USE_REAL_AGENTS=true

# Brave Search API (required for research agent)
BRAVE_API_KEY=your_brave_api_key_here

# Ollama URL (default: http://localhost:11434)
OLLAMA_URL=http://localhost:11434

# Model to use (default: qwen2.5:14b)
OLLAMA_MODEL=qwen2.5:14b
